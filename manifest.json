{
  "required_api_version": "^2.0.0",
  "name": "Meme My Text",
  "description": "A simple extension allowing for easy access to commonly used joke text modifications.",
  "developer_name": "Russell Nairn",
  "icon": "images/icon.png",
  "options": {
    "query_debounce": 0.05
  },
  "preferences": [
    {
      "id": "meme_my_text",
      "type": "keyword",
      "name": "Meme",
      "description": "Text memeing extension",
      "default_value": "meme"
    },
    {
      "id": "ran_cap_enabled",
      "type": "select",
      "name": "Random Capitalization Enabled",
      "default_value": "true",
      "options": [true, false]
    },
    {
      "id": "vaporwave_enabled",
      "type": "select",
      "name": "Vaporwave Enabled",
      "default_value": "true",
      "options": [true, false]
    },
    {
      "id": "reverse_enabled",
      "type": "select",
      "name": "Reverse Enabled",
      "default_value": "true",
      "options": [true, false]
    },
    {
      "id": "pig_latin_enabled",
      "type": "select",
      "name": "Pig Latin Enabled",
      "default_value": "true",
      "options": [true, false]
    }
  ]
}